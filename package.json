{
  "name": "seqpar",
  "version": "1.1.1",
  "description": "seqpar is a batch executor run in sequential & parallel",
  "main": "index.js",
  "bin": {
    "seqpar": "dist/cli.js"
  },
  "files": [
    "package.json",
    "package-lock.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "precommit": "pretty-quick --staged",
    "prepare": "npm run snyk-protect; npm run build",
    "lint": "eslint --cache .",
    "flow": "flow --show-all-errors",
    "test": "NODE_ENV=test nyc mocha --require @babel/register --recursive test/spec",
    "build": "babel src --out-dir dist",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Leko/seqpar.git"
  },
  "keywords": [
    "executable",
    "batch",
    "exec",
    "sequential",
    "parallel",
    "file",
    "js",
    "node"
  ],
  "author": "Leko <leko.noor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Leko/seqpar/issues"
  },
  "homepage": "https://github.com/Leko/seqpar",
  "dependencies": {
    "ascii-progress": "^1.0.5",
    "cli-truncate": "^1.1.0",
    "es6-promisify": "^6.0.0",
    "figures": "^2.0.0",
    "glob": "^7.1.2",
    "inquirer": "^6.0.0",
    "lodash": "^4.17.5",
    "ora": "^3.0.0",
    "string-width": "^3.0.0",
    "yargs": "^14.0.0",
    "snyk": "^1.117.1"
  },
  "yargs": {
    "parse-numbers": true,
    "boolean-negation": true
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/node": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-flow": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "babel-eslint": "^9.0.0",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^3.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^7.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.84.0",
    "husky": "^1.0.0",
    "mocha": "^6.0.0",
    "mock-fs": "^4.4.2",
    "nyc": "^14.0.0",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.5.1",
    "sinon": "^6.3.4"
  },
  "snyk": true
}
